{
  "name": "rent-receipt",
  "private": true,
  "version": "0.1.4",
  "type": "module",
  "scripts": {
    "commit": "git-cz",
    "dev": "yarn app:prebuild && vite",
    "deploy": "gh-pages -d dist",
    "build": "yarn app:prebuild && tsc && vite build",
    "postinstall": "yarn husky install && yarn app:prebuild",
    "pre-commit": "yarn lint:check",
    "eslint:check": "eslint \"*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint \"*.{js,jsx,ts,tsx}\" --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write . --ignore-unknown",
    "lint:check": "yarn eslint:check && yarn prettier:check",
    "lint:fix": "yarn eslint:fix && yarn prettier:write",
    "lint-staged:check": "lint-staged",
    "preview": "vite preview",
    "app:prebuild": "mkver src/version.ts",
    "release": "yarn build && yarn release-it"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.18",
    "@mui/x-date-pickers": "^6.20.0",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "crypto-random-string": "^5.0.0",
    "js-base64": "^3.7.7",
    "moment": "^2.30.1",
    "ordinal": "^1.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-number-format": "^5.3.4",
    "react-qr-code": "^2.0.14",
    "react-router-dom": "^6.23.1",
    "react-to-print": "^2.15.0",
    "to-words": "^4.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "gh-pages": "^6.1.1",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "mkver": "^3.0.2",
    "postcss": "^8.4.37",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "release-it": "^17.3.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
