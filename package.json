{
  "name": "rent-receipt",
  "private": true,
  "version": "0.0.9",
  "type": "module",
  "scripts": {
    "commit": "git-cz",
    "dev": "yarn app:prebuild && vite",
    "deploy": "gh-pages -d dist",
    "build": "yarn app:prebuild && tsc && vite build",
    "postinstall": "yarn husky install && yarn app:prebuild",
    "pre-commit": "yarn lint:check",
    "eslint:check": "eslint \"*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint \"*.{js,jsx,ts,tsx}\" --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write . --ignore-unknown",
    "lint:check": "yarn eslint:check && yarn prettier:check",
    "lint:fix": "yarn eslint:fix && yarn prettier:write",
    "lint-staged:check": "lint-staged",
    "preview": "vite preview",
    "app:prebuild": "mkver src/version.ts",
    "release": "yarn build && yarn release-it"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@mui/x-date-pickers": "^6.18.6",
    "@types/crypto-js": "^4.2.1",
    "crypto-js": "^4.2.0",
    "crypto-random-string": "^5.0.0",
    "moment": "^2.30.1",
    "ordinal": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.3.1",
    "react-qr-code": "^2.0.12",
    "react-router-dom": "^6.21.1",
    "react-to-print": "^2.14.15",
    "to-words": "^4.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mkver": "^2.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "release-it": "^17.0.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
